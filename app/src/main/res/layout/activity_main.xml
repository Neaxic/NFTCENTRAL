<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/my_toolbar"
        style="@style/Widget.MaterialComponents.Toolbar.Primary"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="JAVELIN" />


    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:layout_width="39dp"
        android:layout_height="39dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="588dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:roundPercent="1"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.706" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_toolbar"
        app:layout_constraintVertical_bias="0.714" />

    <androidx.constraintlayout.utils.widget.MockView
        android:id="@+id/mockView2"
        android:layout_width="372dp"
        android:layout_height="177dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="329dp"
        android:layout_height="20dp"
        android:layout_marginTop="48dp"
        android:layout_marginBottom="18dp"
        android:text="0xb504439D29220A07fB5efd6D881df671934C3B51"
        android:textSize="13sp"
        android:translationY="5dp"
        app:layout_constraintBottom_toTopOf="@+id/mockView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.804"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_toolbar"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="303dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="151dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/mockView2"
        app:layout_constraintEnd_toEndOf="@+id/mockView2"
        app:layout_constraintStart_toStartOf="@+id/mockView2"
        app:layout_constraintTop_toTopOf="@+id/mockView2"
        tools:text="@tools:sample/date/ddmmyy" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="96dp"
        android:layout_height="30dp"
        android:text="5,28167 ETH"
        app:layout_constraintBottom_toBottomOf="@+id/mockView2"
        app:layout_constraintEnd_toEndOf="@+id/mockView2"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/mockView2"
        app:layout_constraintTop_toTopOf="@+id/mockView2"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="77dp"
        android:layout_height="14dp"
        android:layout_marginStart="136dp"
        android:layout_marginEnd="162dp"
        android:text="20% (0,02 ETH)"
        android:textSize="10sp"
        android:translationX="-3dp"
        android:translationY="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/mockView2"
        app:layout_constraintEnd_toEndOf="@+id/mockView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/mockView2"
        app:layout_constraintTop_toTopOf="@+id/mockView2"
        app:layout_constraintVertical_bias="0.073" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PORTFOLIO"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="71dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="27dp"
        android:layout_height="25dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/mockView2"
        app:layout_constraintEnd_toEndOf="@+id/mockView2"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="@+id/mockView2"
        app:layout_constraintTop_toTopOf="@+id/mockView2"
        app:layout_constraintVertical_bias="0.072"
        app:srcCompat="@android:drawable/arrow_up_float"
        tools:srcCompat="@drawable/ic_baseline_arrow_drop_up_24" />

</androidx.constraintlayout.widget.ConstraintLayout>